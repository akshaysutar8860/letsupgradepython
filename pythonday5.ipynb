        "k=list(range(1,100))\n",
        "def fun(x):\n",
        "  if x%3==0 and x%5==0:\n",
        "    return True\n",
        "  else:\n",
        "    return False\n",
        "\n",
        "r=filter(fun,k)\n",
        "\n",
        "for i in r:\n",
        "  print(i)\n",
